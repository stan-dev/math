[BASIC]
include-naming-hint=yes
method-rgx=[A-Z_][A-Za-z0-9]{2,49}$
function-rgx=[A-Z_][A-Za-z0-9]{2,49}$|main|unicode_escape_decode
const-rgx=[a-zA-Z_][A-Za-z0-9_]{2,49}$
variable-rgx=[a-z_][a-z0-9_]{0,49}$
argument-rgx=[a-z_][a-z0-9_]{0,49}$
class-rgx=[A-Z_][a-zA-Z0-9]+$|basestring|unicode|long|xrange

[MESSAGES CONTROL]
disable=
  global-statement,
  multiple-statements,
  missing-docstring,
  no-else-return,
  no-self-use,
  consider-merging-isinstance,
  bad-continuation,
  fixme,
  bad-option-value,
  anomalous-unicode-escape-in-string,
  unused-argument,
  useless-object-inheritance,
  consider-using-dict-comprehension,
  consider-using-in,
  unnecessary-pass

[REPORTS]
output-format=colorized
reports=no
score=no

[FORMAT]
indent-string='  '
indent-after-paren=4
max-module-lines=10000

[DESIGN]
max-locals=25
max-line-length=100
max-attributes=10
max-branches=30
max-args=20
max-statements=75
max-returns=10
min-public-methods=0
max-bool-expr=10
